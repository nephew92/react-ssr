    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "nock": "^13.3.2",
    "node-cache": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-router-dom": "^5.3.4",
    "reactstrap": "^9.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack-merge": "^5.9.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.22.10",

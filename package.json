{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Simple SSR example using React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=$NODE_ENV --node-env=$NODE_ENV --env $NODE_ENV",
    "build:theme": "yarn build --config webpack/theme.js",
    "build:server": "yarn build --config webpack/server.js",
    "start:server": "node build/server/bundle.js",
    "dev": "yarn build:theme && yarn build:server && yarn start:server",
    "dev:server": "yarn build:server && yarn start:server",
    "watch": "nodemon -e js,jsx,json -i build/ -x yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajgaur98/react-ssr.git"
  },
  "keywords": [
    "react",
    "ssr",
    "react-ssr"
  ],
  "author": "Rajkumar Gaur rajgaur98@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajgaur98/react-ssr/issues"
  },
  "homepage": "https://github.com/rajgaur98/react-ssr#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.1",
